[buildout]
versions=versions
package-name = ${pdn}
package-extras=
extensions=buildout.minitagificator mr.developer
eggs =
    \${buildout:package-name} \${buildout:package-extras}
    zest.releaser
parts = script
develop = .
[test]
recipe = zc.recipe.testrunner
eggs = \${buildout:package-name} \${buildout:package-extras}
[script]
recipe=minitage.recipe.scripts
eggs=\${buildout:eggs}
interpreter=zopepy
env-file=\${buildout:directory}/test.env
[versions]
[sources]
